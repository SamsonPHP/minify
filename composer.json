{
  "name": "samsonphp/minify",
  "description": "SamsonPHP minification module",
  "keywords": [
    "minification",
    "js minification",
    "css minification"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    }
  ],
  "replace": {
    "samsonos/php_minify": "*"
  },
  "require": {
    "samsonphp/event": "1.*",
    "natxet/CssMin": "3.*",
    "tedivm/jshrink": "~1.0",
    "samsonphp/resource": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "samsonphp\\minify\\": "src/"
    }
  }
}
